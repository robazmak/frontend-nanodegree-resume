                                                                    //In this quiz you are given an array of numbers. 
                                                                    //Can you increase the value of the last number in the array by one?

var sampleArray = [0,0,7,10,12,1,2,3,3];

var incrementLastArrayElement = function(sampleArray) {
    var newArray = [];
    var sum = sampleArray[sampleArray.length - 1] + 1;                //sum one to sampleArray last element
    sampleArray.pop();                                                //remove sampleArray last element & store it on sum
    sum = sampleArray.push(sum);                                      //adds last element addon to original array
    return sampleArray;
};

console.log(incrementLastArrayElement(sampleArray));
